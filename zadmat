package sample;

public class matma {
    public static void main(String [] args) {
        //wpisujemy w tym miejscu dane do obliczenia kolejno a, b, c,
        int a = 2;
        int b = 10;
        int wynik;
        //wzór skróconego mnożenia
        wynik = (a*a)+(2*a*b)+(b*b);
        System.out.println("wynik= " + wynik);
        // finito

    }
} 
